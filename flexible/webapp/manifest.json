{
  "_version": "1.49.0",
  "sap.app": {
    "id": "flexible",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.10.2",
      "toolsId": "26fc785a-8e0b-4bb3-b1d2-ea0dd95b3d1a"
    },
    "dataSources": {
      "mainService": {
        "uri": "/northwind/northwind.svc",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.115.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "flexible.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "flexible.view",
        "controlAggregation": "pages",
        "controlId": "fcl",
        "clearControlAggregation": false,
        "transition": "slide"
      },
      "routes": [
        {
          "name": "View1",
          "pattern": ":layout:",
          "target": [
            "TargetView1"
          ]
        },
        {
					"pattern": "detail/{product}/{layout}",
					"name": "detail",
					"target": [
						"View1",
						"detail"
					]
				}
      ],
      "targets": {
        "TargetView1": {
          "viewId": "View1",
          "viewName": "View1",
          "controlAggregation": "beginColumnPages"
        },
         "detail": {
          "viewId": "detail",
          "viewName": "detail",
          "controlAggregation": "midColumnPages"
        }
      }
    },
    "rootView": {
      "viewName": "flexible.view.App",
      "type": "XML",
      "async": true,
      "id": "fcl"
    }
  }
}
